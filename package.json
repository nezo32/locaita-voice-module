{
  "name": "nyra",
  "version": "1.0.0",
  "description": "A NyraAI using Deepseek.",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start:discord": "node --experimental-specifier-resolution=node dist/discord/index.js",
    "start:telegram": "node --experimental-specifier-resolution=node dist/telegram/index.js",
    "dev:discord": "tsx watch src/discord/index.ts",
    "dev:telegram": "tsx watch src/telegram/index.ts",
    "discord:deploy": "tsx src/discord/api/deploy.ts",
    "supabase:generate": "supabase gen types typescript --project-id tjxtetjuurjtvleudltd > ./src/types/database.types.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "@gradio/client": "^1.17.0",
    "@supabase/supabase-js": "^2.49.4",
    "axios": "^1.8.4",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "fluent-ffmpeg": "^2.1.3",
    "node-cache": "^5.1.2",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.95.1",
    "opusscript": "^0.1.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prism-media": "^1.3.5",
    "supabase": "^2.22.12",
    "throttle-debounce": "^5.0.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.14.1",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/throttle-debounce": "^5.0.2",
    "tsx": "^4.19.3"
  }
}
